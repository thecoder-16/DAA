#include<bits/stdc++.h>
using namespace std;

int diff(int a,int b)
{
    if(a>b)
    {
        return a-b;
    }
    else 
    return b-a;
}

int main()
{
    int T;
    cin>>T;
    for(int i=0;i<T;i++)
    {
        int n,key,count=0;
        cin>>n;
        int arr[n];
        for(int i=0;i<n;i++)
        {
            cin>>arr[i];
        }
        cin>>key;
        for(int i=0;i<n;i++)
        {
            for(int j=i+1;j<n;j++)
            {
                if(diff(arr[i],arr[j])==key)
                {
                    count++;
                }
            }
        }
        cout<<count;
    }
    
}
